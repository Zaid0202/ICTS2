{
    "_version": "1.59.0",
    "sap.app": {
        "id": "internal",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.14.2",
            "toolsId": "f2e5cd40-d78d-44f0-bb3b-4ea8ef52e0cb"
        },
        "dataSources": {
            "ZBTP_ICTS_SRV_SRV1": {
                "uri": "/sap/opu/odata/SAP/ZBTP_ICTS_SRV_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/ZBTP_ICTS_SRV_SRV1/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "v2": {
                "uri": "/odata/v2/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "ZEMAIL_NOTIFICATION_SRV": {
                "uri": "/sap/opu/odata/SAP/ZEMAIL_NOTIFICATION_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/ZEMAIL_NOTIFICATION_SRV/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "ZACCOMODATION_APPS_SRV": {
                "uri": "/sap/opu/odata/SAP/ZACCOMODATION_APPS_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/ZACCOMODATION_APPS_SRV/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.126.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "internal.i18n.i18n"
                }
            },
            "navList": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/navList.json"
            },
            "localData": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/localData.json"
            },
            "endsPoints": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/endsPoints.json"
            },
            "SF": {
                "preload": true,
                "dataSource": "v2",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "": {
                "dataSource": "ZBTP_ICTS_SRV_SRV1",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "ZEMAIL_NOTIFICATION_SRV": {
                "dataSource": "ZEMAIL_NOTIFICATION_SRV",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "ZACCOMODATION_APPS_SRV": {
                "dataSource": "ZACCOMODATION_APPS_SRV",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "internal.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteHome",
                    "pattern": ":?query:",
                    "target": [
                        "TargetHome"
                    ]
                },
                {
                    "name": "RouteMyTasks",
                    "pattern": "RouteMyTasks",
                    "target": [
                        "TargetMyTasks"
                    ]
                },
                {
                    "name": "RouteNewRequest",
                    "pattern": "RouteNewRequest",
                    "target": [
                        "TargetNewRequest"
                    ]
                },
                {
                    "name": "RouteMyRequestStatus",
                    "pattern": "RouteMyRequestStatus",
                    "target": [
                        "TargetMyRequestStatus"
                    ]
                },
                {
                    "name": "RouteProcessedByMe",
                    "pattern": "RouteProcessedByMe",
                    "target": [
                        "TargetProcessedByMe"
                    ]
                },
                {
                    "name": "RouteDashboard",
                    "pattern": "RouteDashboard",
                    "target": [
                        "TargetDashboard"
                    ]
                },
                {
                    "name": "RouteSettings",
                    "pattern": "RouteSettings",
                    "target": [
                        "TargetSettings"
                    ]
                },
                {
                    "name": "RouteSettingsAssignees",
                    "pattern": "RouteSettingsAssignees",
                    "target": [
                        "TargetSettingsAssignees"
                    ]
                },
                {
                    "name": "RouteSettingsApprovals",
                    "pattern": "RouteSettingsApprovals",
                    "target": [
                        "TargetSettingsApprovals"
                    ]
                },
                {
                    "name": "RouteRequestStatusForm",
                    "pattern": "RouteRequestStatusForm/:Id:",
                    "target": [
                        "TargetRequestStatusForm"
                    ]
                }
            ],
            "targets": {
                "TargetHome": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Home",
                    "viewName": "Home"
                },
                "TargetMyTasks": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "MyTasks",
                    "viewName": "MyTasks"
                },
                "TargetNewRequest": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "NewRequest",
                    "viewName": "NewRequest"
                },
                "TargetMyRequestStatus": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "MyRequestStatus",
                    "viewName": "MyRequestStatus"
                },
                "TargetProcessedByMe": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ProcessedByMe",
                    "viewName": "ProcessedByMe"
                },
                "TargetDashboard": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Dashboard",
                    "viewName": "Dashboard"
                },
                "TargetSettings": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Settings",
                    "viewName": "Settings"
                },
                "TargetSettingsAssignees": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "SettingsAssignees",
                    "viewName": "SettingsAssignees"
                },
                "TargetSettingsApprovals": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "SettingsApprovals",
                    "viewName": "SettingsApprovals"
                },
                "TargetRequestStatusForm": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "RequestStatusForm",
                    "viewName": "RequestStatusForm"
                }
            }
        },
        "rootView": {
            "viewName": "internal.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}